<div id="foursquare">
	<a href="/foursquare/signout"> Sign out of foursquare </a>
	<div id="foursquareprofile">
		Email: <%=session[:foursquare][:info][:email]%>
		Name: <%=session[:foursquare][:info][:name]%>
		Description: <%=session[:foursquare][:info][:description]%>
		Location: <%=session[:foursquare][:info][:location]%>
		<img src ="<%= session[:foursquare][:info][:image][:prefix]%>128x128<%= session[:foursquare][:info][:image][:suffix] %>">
	</div>
	<div id="foursquaresearch">
		<form action="/foursquare/search" method="post">
			<input type="text" name="query" placeholder="Put search term here">
			<input type="submit" value="Search!">
		</form>
	</div>
	<div id="searchresults">
	</div>
</div>