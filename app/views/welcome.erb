<p>Check out the <a href="google">Google APIs</a></p>
<% if session[:twitter] %>
	<%= erb :twitter, :layout => false %>
<% else %>
	<p>Sign into <a href="/auth/twitter">Twitter</a></p>
<% end %>

<% if session[:soundcloud] %>
	<%= erb :soundcloud, :layout => false %>
<% else %>
	<p>Sign into <a href="/auth/soundcloud">Soundcloud</a></p>
<% end %>